<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.daqsoft.commonnanning.MainActivity">

    <FrameLayout
        android:id="@+id/fl_tab_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/dp_52" />

    <io.agora.yview.tablayout.CommonTabLayout
        android:id="@+id/tab_bottom"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_52"
        android:layout_alignParentBottom="true"
        android:background="@color/main_white"
        app:tl_textSelectColor="@color/main_default"
        app:tl_textUnselectColor="@color/main_gray"
        app:tl_underline_color="@color/main_bg"
        app:tl_underline_gravity="TOP"
        app:tl_underline_height="@dimen/dp_1" />

</RelativeLayout>
